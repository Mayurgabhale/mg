// ── Denver-only redirect settings ─────────────────────────────────────────────
const DENVER_PARTITION = "US.CO.OBS";
const DENVER_EXTERNAL_URL = "http://10.199.22.57:3012";




useEffect(() => {
  if (partition === DENVER_PARTITION) {
    window.location.replace(DENVER_EXTERNAL_URL);
  }
}, [partition]);





if (partition === DENVER_PARTITION) return null;