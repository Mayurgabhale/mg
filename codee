const getIsoDate = (r) =>
  (r?.SwipeDate && r.SwipeDate.slice(0, 10))
  || (r?.LocaleMessageTime && r.LocaleMessageTime.slice(0, 10))
  || '';



.....



all.sort((a, b) => (a.LocaleMessageTime || '').localeCompare(b.LocaleMessageTime || ''));





