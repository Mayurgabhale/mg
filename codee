// inside Dashboard component
const [data, setData] = useState(null);
const [mode, setMode] = useState('live');

const handleSnapshot = snapshotJson => {
  setData(snapshotJson);
  setMode('snapshot');
};

const handleLive = () => {
  setMode('live');
  setData(liveData);
};

// in return JSX
<Header onSnapshot={handleSnapshot} onLive={handleLive} />