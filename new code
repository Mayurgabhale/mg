{suggestions.map(emp => (
  <li key={emp.EmployeeID ?? emp.EmpName} onClick={() => pickSuggestion(emp)}>
    {emp.EmpName}{emp.EmployeeID && ` (${emp.EmployeeID})`}
  </li>
))}







{history.map(emp => (
  <li key={emp.EmployeeID ?? emp.EmpName} onClick={() => pickHistory(emp)}>
    {emp.EmpName}{emp.EmployeeID && ` (${emp.EmployeeID})`}
  </li>
))}